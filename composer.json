{
    "name": "angellco/fffields",
    "description": "Fabulous front-end fields for Craft.",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "fffields",
        "fields"
    ],
    "support": {
        "docs": "https://github.com/angell-co/fffields/blob/master/README.md",
        "issues": "https://github.com/angell-co/fffields/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Angell & Co",
            "homepage": "https://angell.io"
        }
    ],
    "require": {
        "craftcms/cms": "^3.1.0"
    },
    "autoload": {
        "psr-4": {
          "angellco\\fffields\\": "src/"
        }
    },
    "extra": {
        "name": "FFFields",
        "handle": "fffields",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/angell-co/fffields/master/CHANGELOG.md",
        "components": {
            "fieldConfig": "angellco\\fffields\\services\\FieldConfig",
            "render": "angellco\\fffields\\services\\Render"
        },
        "class": "angellco\\fffields\\FFFields"
    }
}
